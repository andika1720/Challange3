<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.challangebinar3.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_detailFragmentMenu"
            app:destination="@id/detailFragmentMenu" />
        <action
            android:id="@+id/action_homeFragment_to_fragmentCart"
            app:destination="@id/fragmentCart" />
    </fragment>
    <fragment
        android:id="@+id/detailFragmentMenu"
        android:name="com.example.challangebinar3.ui.fragment.DetailFragmentMenu"
        android:label="DetailFragmentMenu">
        <action
            android:id="@+id/action_detailFragmentMenu_to_fragmentCart"
            app:destination="@id/fragmentCart" />
        <action
            android:id="@+id/action_detailFragmentMenu_to_fragmentCart2"
            app:destination="@id/fragmentCart" />
    </fragment>
    <fragment
        android:id="@+id/fragmentProfile"
        android:name="com.example.challangebinar3.ui.fragment.FragmentProfile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_fragmentProfile_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_fragmentProfile_to_userLogin"
            app:destination="@id/userLogin" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCart"
        android:name="com.example.challangebinar3.ui.fragment.FragmentCart"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart">
        <action
            android:id="@+id/action_fragmentCart_to_fragmentKonfirmasiPesanan"
            app:destination="@id/fragmentKonfirmasiPesanan" />
        <action
            android:id="@+id/action_fragmentCart_to_fragmentKonfirmasiPesanan2"
            app:destination="@id/fragmentKonfirmasiPesanan" />
    </fragment>
    <fragment
        android:id="@+id/fragmentKonfirmasiPesanan"
        android:name="com.example.challangebinar3.ui.fragment.FragmentKonfirmasiPesanan"
        android:label="fragment_konfirmasi_pesanan"
        tools:layout="@layout/fragment_konfirmasi_pesanan" />
    <activity
        android:id="@+id/userRegister"
        android:name="com.example.challangebinar3.ui.activity.UserRegister"
        android:label="activity_user_register"
        tools:layout="@layout/activity_user_register" />
    <activity
        android:id="@+id/userLogin"
        android:name="com.example.challangebinar3.ui.activity.UserLogin"
        android:label="activity_user_login"
        tools:layout="@layout/activity_user_login" />
</navigation>